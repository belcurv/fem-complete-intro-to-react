{
	"extends": [
		"airbnb",
		"prettier",
		"prettier/react"
	],
  "parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": 0,
    "no-undef": 0
  }
}
